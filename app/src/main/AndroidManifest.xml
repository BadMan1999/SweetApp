                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.AddChaletActivity" />
        <activity android:name=".Activity.NewPhoneNumberActivity" />
