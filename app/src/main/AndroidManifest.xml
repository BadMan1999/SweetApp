        <activity android:name=".Activity.ForgetEmailActivity" />
        <activity android:name=".Activity.ChaletOwnerMainActivity" />
        <activity android:name=".Activity.TenantLoginActivity" />
        <activity android:name=".Activity.ChalatOwnetLoginActivity" />
        <activity android:name=".Activity.ChaletOwnerSingupActivity" />
        <activity android:name=".Activity.TenantSingUpOrLoginActivity" />
        <activity android:name=".Activity.ChaletOwnerSingUpOrLoginActivity" />
        <activity android:name=".Activity.ChooseTypeActivity" />
        <activity android:name=".Activity.TenantSingUpActivity" />
        <activity android:name=".Activity.SplashActivity" />
        <activity android:name=".Activity.MainActivity" />
